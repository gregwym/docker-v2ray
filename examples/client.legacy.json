{
  "log": {
    "loglevel": "info"
  },
  "dns": {
    "servers": ["8.8.8.8", "8.8.4.4"]
  },
  "inbounds": [
    {
      "port": 3456,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    },
    {
      "port": 3457,
      "listen": "127.0.0.1",
      "protocol": "http",
      "settings": {
        "timeout": 360
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    }
  ],
  "outbound": {
    "protocol": "vmess",
    "settings": {
      "vnext": [
        {
          "address": "example.com",
          "port": 443,
          "users": [
            {
              "id": "client-uuid",
              "level": 1,
              "security": "auto",
              "alterId": 64
            }
          ]
        }
      ]
    },
    "mux": {
      "enabled": true
    },
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
        "path": "/graphql",
        "headers": {
          "Host": "example.com"
        }
      },
      "security": "tls"
    }
  },
  "outboundDetour": [
    {
      "protocol": "freedom",
      "tag": "direct",
      "settings": {}
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "rules": [
        {
          "type": "field",
          "port": "54-79,81-442,444-464,466-586,588-992,994-65535",
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "domain": ["regexp:apple\\..+", "icloud.com"],
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "domain": ["geosite:cn", "geosite:speedtest"],
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "ip": [
            "0.0.0.0/8",
            "10.0.0.0/8",
            "100.64.0.0/10",
            "127.0.0.0/8",
            "169.254.0.0/16",
            "172.16.0.0/12",
            "192.0.0.0/24",
            "192.0.2.0/24",
            "192.168.0.0/16",
            "198.18.0.0/15",
            "198.51.100.0/24",
            "203.0.113.0/24",
            "::1/128",
            "fc00::/7",
            "fe80::/10"
          ],
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "ip": ["geoip:cn", "geoip:private"],
          "outboundTag": "direct"
        }
      ]
    }
  }
}
